<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="id.usereal.eventdicoding.ui.home.HomeFragment"
        android:title="@string/home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_upcoming"
        android:name="id.usereal.eventdicoding.ui.upcoming.UpcomingFragment"
        android:title="@string/title_upcoming"
        tools:layout="@layout/fragment_upcoming" >
    </fragment>

    <fragment
        android:id="@+id/navigation_finished"
        android:name="id.usereal.eventdicoding.ui.finished.FinishedFragment"
        android:title="@string/title_finished"
        tools:layout="@layout/fragment_finished" >
    </fragment>


</navigation>
